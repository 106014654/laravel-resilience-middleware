{
    "name": "onelap/laravel-resilience-middleware",
    "description": "Laravel弹性中间件包 - 提供限流、熔断器、服务降级等微服务保护功能",
    "type": "library",
    "keywords": [
        "laravel",
        "middleware",
        "resilience",
        "rate-limiting",
        "circuit-breaker",
        "service-degradation",
        "microservices",
        "reliability",
        "fault-tolerance"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "OneLap Team",
            "email": "dev@onelap.com",
            "homepage": "https://onelap.com"
        }
    ],
    "homepage": "https://github.com/onelap/laravel-resilience-middleware",
    "support": {
        "issues": "https://github.com/onelap/laravel-resilience-middleware/issues",
        "source": "https://github.com/onelap/laravel-resilience-middleware",
        "docs": "https://github.com/onelap/laravel-resilience-middleware/wiki"
    },
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/http": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/redis": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/cache": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/database": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0|^10.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0|^9.0",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "OneLap\\LaravelResilienceMiddleware\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OneLap\\LaravelResilienceMiddleware\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OneLap\\LaravelResilienceMiddleware\\ResilienceMiddlewareServiceProvider"
            ],
            "aliases": {
                "SystemMonitor": "OneLap\\LaravelResilienceMiddleware\\Facades\\SystemMonitor"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}